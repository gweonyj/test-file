   ],
   "source": [
    "import pandas as pd\n",
    "\n",
    "temp = pd.Series([-20, -10, 10, 20])\n",
    "\n",
    "print(temp)\n",
    "print(temp[0]) # 첫번째 값 출력\n",
    "\n",
    "temp = pd.Series([-20, -10, 10, 20], index = ['Jan', 'Feb', 'Mar', 'Apr'])  # index 지정\n",
    "\n",
    "print(temp['Jan']) # index 'Jan'에 해당되는 데이터 출력\n",
    "\n",
    "\n"
